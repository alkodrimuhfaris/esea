est(\n    'partner-test',\n    'Pilih jenis mitra yang benar!',\n    (value) => {\n      value = value === 'Nelayan' || value === 'Penjual';\n      return value;\n    },\n  ),\n});\n\nexport default function WorkingTogether() {\n  const { width, xs, sm, md, lg, xl } = useWindowDimensions();\n  const [sideLength, setSideLength] = React.useState(0);\n\n  React.useEffect(() => {\n    let side = sideLength;\n    if (xs) {\n      side = 0;\n    } else if (sm) {\n      side = (width - 576) / 2;\n    } else if (md) {\n      side = (width - 768) / 2;\n    } else if (lg) {\n      side = (width - 992) / 2;\n    } else if (xl) {\n      side = (width - 1200) / 2;\n    }\n    setSideLength(side);\n  }, [width]);\n\n  const submit = (values) => {\n    console.log(values);\n  };\n\n  const initialValue = {\n    name: '',\n    email: '',\n    phone: '',\n  };\n\n  return (\n    <div className=\"parent position-relative mb-5\">\n      {/* left background image */}\n      <div\n        className=\"left-background position-absolute\"\n        style={{ width: sideLength }}\n      >\n        <div className=\"d-flex justify-content-center align-items-center background-wrapper position-relative\">\n          <div\n            style={{ width: sideLength + 2 }}\n            className=\"background-left-cover position-absolute\"\n          >\n            &nbsp;\n          </div>\n          <img\n            src={leftBackground}\n            className=\"image-cover\"\n            alt=\"left-background\"\n          />\n        </div>\n      </div>\n      {/* right background image */}\n      <div\n        className=\"right-background position-absolute\"\n        style={{ width: sideLength }}\n      >\n        <div className=\"d-flex justify-content-center align-items-center background-wrapper position-relative\">\n          <div\n            style={{ width: sideLength + 2 }}\n            className=\"background-right-cover position-absolute\"\n          >\n            &nbsp;\n          </div>\n          <img\n            src={rightBackground}\n            className=\"image-cover\"\n            alt=\"right-background\"\n          />\n        </div>\n      </div>\n      <Container className=\"pt-5 d-flex container-partner flex-column justify-content-center align-items-center\">\n        <div className=\"d-flex my-5 align-items-center partner-title justify-content-center\">\n          <h4 className=\"kumbh-sans text-center\">Our Partners</h4>\n        </div>\n\n        <h2 className=\"kumbh-sans text-center\">Mulai Kerja Sama Dengan Kami</h2>\n        <Formik\n          initialValues={initialValue}\n          validationSchema={schema}\n          validateOnBlur\n          onSubmit={(values) => {\n            submit(values);\n          }}\n        >\n          {(props) => {\n            const {\n              touched,\n              errors,\n              handleSubmit,\n              values,\n              handleChange,\n              handleBlur,\n              setFieldValue,\n            } = props;\n            return (\n              <Form\n                onSubmit={handleSubmit}\n                className={`montserrat partner-form ${md ? 'w-100' : 'w-60'}`}\n              >\n                <FormGroup className=\"mt-1\">\n                  <Label for=\"name\">Nama</Label>\n                  <Input\n                    tag={Field}\n                    value={values.name}\n                    onChange={handleChange}\n                    type=\"name\"\n                    name=\"name\"\n                    id=\"name\"\n                    placeholder=\"Nama\"\n                    onBlur={handleBlur}\n                  />\n                  {errors.name || touched.name ? (\n                    <text color=\"danger\" className=\"error-form\">\n                      {errors.name}\n                    </text>\n                  ) : null}\n                </FormGroup>\n\n                <FormGroup className=\"mt-1\">\n                  <Label for=\"email\">E-mail</Label>\n                  <Input\n                    tag={Field}\n                    value={values.email}\n                    onChange={handleChange}\n                    type=\"email\"\n                    name=\"email\"\n                    id=\"email\"\n                    placeholder=\"E-mail\"\n                    onBlur={handleBlur}\n                  />\n                  {errors.email || touched.email ? (\n                    <text color=\"danger\" className=\"error-form\">\n                      {errors.email}\n                    </text>\n                  ) : null}\n                </FormGroup>\n\n                <FormGroup className=\"mt-1\">\n                  <Label for=\"phone\">Telepon</Label>\n                  <Input\n                    tag={Field}\n                    value={values.phone}\n                    onChange={handleChange}\n                    type=\"text\"\n                    name=\"phone\"\n                    id=\"phone\"\n                    placeholder=\"Telepon\"\n                    onBlur={handleBlur}\n                  />\n                  {errors.phone || touched.phone ? (\n                    <text color=\"danger\" className=\"error-form\">\n                      {errors.phone}\n                    </text>\n                  ) : null}\n                </FormGroup>\n\n                <Col sm={12} className=\"d-flex\">\n                  {['Penjual', 'Nelayan'].map((item, i) => (\n                    <FormGroup className={i > 0 && 'ml-4'} check>\n                      <Label check>\n                        <Input\n                          type=\"radio\"\n                          name=\"partner\"\n                          id={`partner-${item}`}\n                          value={item}\n                          checked={item === values.partner}\n                          onChange={(e) =>\n                            setFieldValue('partner', e.target.value)\n                          }\n                        />\n                        <span>{`Mitra ${item}`}</span>\n                      </Label>\n                    </FormGroup>\n                  ))}\n                </Col>\n                <Col sm={12}>\n                  {errors.partner || touched.partner ? (\n                    <text color=\"danger\" className=\"error-form\">\n                      {errors.partner}\n                    </text>\n                  ) : null}\n                </Col>\n\n                <div className=\"my-3 d-flex justify-content-center\">\n                  <Button className=\"font-bold btn-esea-main\" type=\"submit\">\n                    Kirim\n                  </Button>\n                </div>\n              </Form>\n            );\n          }}\n        </Formik>\n      </Container>\n    </div>\n  );\n}\n","/home/azis/Project/projects2021/esea2/esea/src/Components/Products/Products.js",[],"/home/azis/Project/projects2021/esea2/esea/src/Helpers/useWindowDimension.js",[],"/home/azis/Project/projects2021/esea2/esea/src/Helpers/responsiveCarousel.js",[],"/home/azis/Project/projects2021/esea2/esea/src/Components/Components/ProductCard.js",[],"/home/azis/Project/projects2021/esea2/esea/src/Helpers/currencyFormat.js",[],"/home/azis/Project/projects2021/esea2/esea/src/Components/WeAreEsea/index.js",[],"/home/azis/Project/projects2021/esea2/esea/src/Components/AboutUs/index.js",[],"/home/azis/Project/projects2021/esea2/esea/src/Components/TitleBox/index.js",[],"/home/azis/Project/projects2021/esea2/esea/src/Components/Footer/index.js",[],{"ruleId":"70","replacedBy":"71"},{"ruleId":"72","replacedBy":"73"},{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","severity":1,"message":"81","line":65,"column":5,"nodeType":"82","messageId":"83","endLine":65,"endColumn":16},"lines-around-directive",["84"],"global-require",[],"no-buffer-constructor",[],"no-new-require",[],"no-path-concat",[],"no-console","Unexpected console statement.","MemberExpr